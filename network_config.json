{
  "networking": {
    "version": "2.0.0",
    "description": "OMEGA_X Networking Components Configuration"
  },
  "tcp_client": {
    "default_host": "127.0.0.1",
    "default_port": 4444,
    "ssl_enabled": false,
    "proxy_support": true,
    "stealth_mode": true,
    "heartbeat_interval": 60,
    "buffer_size": 4096,
    "timeout": 30,
    "max_retries": 3
  },
  "tcp_server": {
    "default_host": "0.0.0.0",
    "default_port": 4444,
    "ssl_enabled": false,
    "max_clients": 100,
    "client_timeout": 300,
    "buffer_size": 4096,
    "log_level": "info",
    "exfiltration_dir": "exfiltrated_data",
    "log_dir": "server_logs"
  },
  "tcp_proxy": {
    "default_listen_host": "0.0.0.0",
    "default_listen_port": 8080,
    "ssl_enabled": false,
    "intercept_enabled": false,
    "buffer_size": 4096,
    "max_connections": 1000,
    "traffic_logging": true,
    "log_level": "info"
  },
  "udp_client": {
    "default_host": "127.0.0.1",
    "default_port": 4445,
    "broadcast_enabled": false,
    "multicast_enabled": false,
    "buffer_size": 4096,
    "timeout": 5.0,
    "max_packet_size": 65507,
    "packet_fragmentation": true,
    "error_handling": true
  },
  "security": {
    "encryption_enabled": true,
    "encryption_key": "omega_x_secure_key_2024",
    "signature_verification": false,
    "certificate_validation": false,
    "anti_replay_protection": true,
    "rate_limiting": true,
    "max_requests_per_minute": 1000
  },
  "performance": {
    "multithreading": true,
    "async_operations": true,
    "connection_pooling": true,
    "buffer_pooling": true,
    "zero_copy_operations": false,
    "memory_optimization": true
  },
  "monitoring": {
    "statistics_enabled": true,
    "health_checks": true,
    "metrics_export": false,
    "alerting": false,
    "log_rotation": true,
    "max_log_size": "100MB",
    "log_retention_days": 30
  },
  "protocols": {
    "tcp": {
      "keepalive": true,
      "nodelay": true,
      "reuseaddr": true,
      "backlog": 100
    },
    "udp": {
      "broadcast": false,
      "multicast_ttl": 2,
      "multicast_loop": true,
      "reuseaddr": true
    },
    "ssl": {
      "version": "TLSv1.2",
      "ciphers": "HIGH:!aNULL:!eNULL:!EXPORT:!DES:!RC4:!MD5:!PSK:!SRP:!CAMELLIA",
      "certfile": null,
      "keyfile": null,
      "cafile": null
    }
  },
  "firewall": {
    "enabled": false,
    "rules": [],
    "default_policy": "allow",
    "logging": true
  },
  "load_balancing": {
    "enabled": false,
    "algorithm": "round_robin",
    "servers": [],
    "health_checks": true,
    "session_persistence": false
  },
  "clustering": {
    "enabled": false,
    "nodes": [],
    "discovery_method": "static",
    "heartbeat_interval": 30,
    "failover_enabled": true
  }
}